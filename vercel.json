{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/login.html",
      "dest": "/public/login.html"
    },
    {
      "src": "/login",
      "dest": "/public/login.html"
    },
    {
      "src": "/signup.html",
      "dest": "/public/signup.html"
    },
    {
      "src": "/signup",
      "dest": "/public/signup.html"
    },
    {
      "src": "/dashboard.html",
      "dest": "/public/dashboard.html"
    },
    {
      "src": "/dashboard",
      "dest": "/public/dashboard.html"
    },
    {
      "src": "/test-google-auth.html",
      "dest": "/public/test-google-auth.html"
    },
    {
      "src": "/clear-session.html",
      "dest": "/public/clear-session.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
